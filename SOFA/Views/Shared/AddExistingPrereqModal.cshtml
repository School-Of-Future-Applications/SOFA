@using SOFA.Models.ViewModels

@model AddExistingPreReqViewModel

<div id="add-section-modal" class="modal fade" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content" id="add-section-modal-content">

            @Html.Action("AddExistingPrereq", "ClassBase", new { classBaseId = Model.ClassBaseId })
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {
        $(".add-section-link").click(function (e) {
            e.preventDefault();
            $("#add-section-modal").modal({
                backdrop: 'static',
                keyboard: true
            }, 'show');
            return false;
        });

    });





</script>

